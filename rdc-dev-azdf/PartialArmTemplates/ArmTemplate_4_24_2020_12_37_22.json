{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rdc-dev-azdf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/stage_trigger_min20_schedule')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "stage_trigger_min20_schedule",
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipe_CAMP",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_GeneralLedgerJournals",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_xCAMP",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_SubLedgers",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Expenditures",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Purchasing",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Receivables",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Quotes",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Projects",
							"type": "PipelineReference"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "pipe_Projects_W",
							"type": "PipelineReference"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 120,
						"startTime": "2020-04-23T06:12:00.000Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/stage_20min_00')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Trigger Every 20 Minutes on the 00",
				"annotations": [],
				"runtimeState": "Stopped",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "pipe_GeneralLedger",
							"type": "PipelineReference"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 20,
						"startTime": "2020-04-24T00:00:00.000Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": []
		}
	]
}